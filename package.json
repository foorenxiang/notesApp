{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "electron": "yarn expo-electron start",
    "electron:build-dev": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "electron:build-prod": "yarn electron-webpack && yarn electron-builder --dir -c.mac.identity=null",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "expo": "~41.0.1",
    "expo-status-bar": "~1.0.4",
    "lodash": "^4.17.21",
    "punycode": "^2.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.3.0",
    "react-native-web": "~0.16.3",
    "react-native-webview": "11.6.2",
    "rn-pdf-reader-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.12.74",
    "electron": "^13.0.1",
    "electron-builder": "^22.10.5",
    "esm": "^3.2.25"
  },
  "private": true,
  "homepage": "/notes",
  "name": "rxnotes",
  "description": "RX programming notes",
  "author": {
    "name": "Ren Xiang Foo",
    "url": "http://renxiang.cloud"
  },
  "version": "1.0.0",
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  }
}
